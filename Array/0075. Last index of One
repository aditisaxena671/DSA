// GeeksForGeeks Link : https://practice.geeksforgeeks.org/problems/last-index-of-15847/1

class Solution {
    public int lastIndex( String s) {
        int ans=-1;
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            if(ch=='1'){
                ans=i;
        
            }
        }
        return ans;
        
    }
}
